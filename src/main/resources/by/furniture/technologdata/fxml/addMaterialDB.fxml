<?xml version="1.0" encoding="UTF-8"?>

<?import com.sun.javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="211.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="by.furniture.technologdata.controllers.AddMaterialDBController">
    <children>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="110.0" minWidth="110.0" prefWidth="110.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="488.0" minWidth="10.0" prefWidth="472.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
            <children>
                <Label text="Артикул:">
                    <font>
                        <Font name="System Bold" size="13.0" />
                    </font>
                </Label>
                <Label text="Наименование:" GridPane.rowIndex="1">
                    <font>
                        <Font name="System Bold" size="13.0" />
                    </font>
                </Label>
                <Label text="Длина листа:" GridPane.rowIndex="2">
                    <font>
                        <Font name="System Bold" size="13.0" />
                    </font>
                </Label>
                <Label text="Ширина листа:" GridPane.rowIndex="3">
                    <font>
                        <Font name="System Bold" size="13.0" />
                    </font>
                </Label>
                <Label text="Толщина листа:" GridPane.rowIndex="4">
                    <font>
                        <Font name="System Bold" size="13.0" />
                    </font>
                </Label>
                <TextField fx:id="articleTextField" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                <TextField fx:id="nameTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <TextField fx:id="lengthField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <TextField fx:id="widthField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <TextField fx:id="thicknessField" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            </children>
        </GridPane>
        <HBox alignment="CENTER_RIGHT" maxHeight="40.0" minHeight="40.0" prefHeight="40.0" prefWidth="600.0" spacing="10.0" VBox.vgrow="ALWAYS">
            <children>
                <Button fx:id="okButton" maxWidth="100.0" minWidth="100.0" mnemonicParsing="false" onAction="#onOkButtonClick" prefWidth="100.0" text="OK" />
                <Button fx:id="cancelButton" cancelButton="true" maxWidth="100.0" minWidth="100.0" mnemonicParsing="false" onAction="#onCancelButtonClick" prefWidth="100.0" text="Отмена" />
            </children>
            <padding>
                <Insets right="10.0" />
            </padding>
        </HBox>
    </children>
</VBox>
